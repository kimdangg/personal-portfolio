<div class="well">
  <%= render "layouts/errors", :object => @skill %>

  <%= form_for [@skill, @project] do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="form-group">
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
    <div class="form-group">
      <%= f.label :link %>
      <%= f.text_field :link %>
    </div>
    <%= f.submit %>
  <% end %>
</div>
