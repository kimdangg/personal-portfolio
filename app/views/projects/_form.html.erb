<div class="well well-form">
  <%= render "layouts/errors", :object => @project %>

  <%= form_for [@skill, @project] do |f| %>
    <div class="form-group">
      <%= f.label :name %><br />
      <%= f.text_field :name, :size => "31" %><br />
    </div>
    <div class="form-group">
      <%= f.label :description %><br />
      <%= f.text_area :description, :cols => "30", :rows => "6" %><br />
    </div>
    <div class="form-group">
      <%= f.label :link %><br />
      <%= f.text_field :link, :size => "31" %><br />
    </div>
    <div class="form-group">
      <%= f.label :preview_image %><br />
      <%= f.text_field :preview_image, :size => "31" %><br />
    </div>
    <%= f.submit %>
  <% end %>
</div>
