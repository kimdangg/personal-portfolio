<%= render "layouts/errors", :object => @skill %>

<%= form_for @skill do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <%= f.submit :class => 'btn btn-info' %>
<% end %>
